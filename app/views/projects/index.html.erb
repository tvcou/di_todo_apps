<h1>Projects</h1>
<u1>
  <% @projects.each do |project| %>
    <li>
      <%= link_to project.title, project_path(project.id) %>
      <%= link_to "[edit]", edit_project_path(project.id) %>
      <%= link_to "[Delete]", project_path(project.id), method: :delete %>
    </li>
  <% end %>
  <p><%= link_to "Add New", new_project_path %></p>
</u1>
